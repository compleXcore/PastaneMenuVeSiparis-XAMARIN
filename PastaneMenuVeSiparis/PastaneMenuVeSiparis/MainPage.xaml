<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PastaneMenuVeSiparis.MainPage"
                xmlns:local="clr-namespace:PastaneMenuVeSiparis"
                xmlns:views="clr-namespace:PastaneMenuVeSiparis.Views">

    <FlyoutPage.Flyout>
        <local:FlyoutMenuPage x:Name="menuPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:Anasayfa />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>

</FlyoutPage>
